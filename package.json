{
  "name": "tor-journalism-crawler",
  "version": "1.0.0",
  "description": "A Node.js crawler for journalists to discover content through Tor",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && (node dist/index.js & node dist/api-server.js & wait)",
    "start:crawler": "node dist/index.js",
    "start:api": "node dist/api-server.js",
    "start:scanner": "node dist/scanner-orchestrator.js",
    "start:dev": "npm run build && (tsx src/index.ts & tsx src/api-server.ts & wait)",
    "crawler": "node dist/index.js",
    "crawler:dev": "tsx src/index.ts",
    "api": "node dist/api-server.js",
    "api:dev": "tsx src/api-server.ts",
    "scanner": "node dist/scanner-orchestrator.js",
    "scanner:dev": "tsx src/scanner-orchestrator.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "tor",
    "crawler",
    "journalism",
    "privacy",
    "onion"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "generic-pool": "^3.9.0",
    "pg": "^8.11.3",
    "socks": "^2.8.7",
    "socks-proxy-agent": "^8.0.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
